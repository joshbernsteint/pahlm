{
  "name": "pahlm",
  "displayName": "pahlm",
  "description": "A note-taking languaging combining the ease of use of markdown with the math capabilities of LaTeX.",
  "version": "0.0.1",
  "type": "commonjs",
  "main": "./extension.js",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pahlm",
        "aliases": [
          "PaHLM",
          "pahlm"
        ],
        "extensions": [
          ".plm"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./themes/file_icon.png",
          "dark": "./themes/file_icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "pahlm",
        "scopeName": "text.html.pahlm",
        "path": "./syntaxes/pahlm.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "pahlm.api.compile",
        "title": "Compile"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "pahlm.api.compile",
          "group": "navigation"
        }
      ]
    }
  },
  "dependencies": {
    "html-entities": "^2.4.0",
    "pdf-puppeteer": "^1.1.11"
  }
}
